{
  "name": "backend",
  "version": "1.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "first-install": "yarn install && husky install",
    "start": "nest start",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "check": "npx npm-check -s -u",
    "test": "npx jest",
    "test:unit": "npm run test -- --config ./jest-unit.config.js",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:watch": "npm run test -- --watch --config ./jest-unit.config.js",
    "test:cov": "npm run test -- --coverage --config ./jest-unit.config.js",
    "test:e2e": "npm run test -- --config ./jest-e2e.config.js",
    "start:dev": "nest start --watch",
    "start:debug": "export NODE_ENV=development && ts-node-dev --respawn --transpile-only --inspect=0.0.0.0:9229 --ignore-watch node_modules -r tsconfig-paths/register ./src/main.ts",
    "start:prod": "export NODE_ENV=production && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:staged": "eslint --fix",
    "migration:create": "npx typeorm migration:create ./typeorm-migrations/$npm_config_name -o",
    "migration:generate": "npx typeorm migration:generate -d ./dist/typeorm.migration-config.js ./typeorm-migrations/$npm_config_name -o",
    "migration:run": "npm run build && npx typeorm migration:run -d ./dist/typeorm.migration-config.js",
    "migration:revert": "npm run build && npx typeorm migration:revert -d ./dist/typeorm.migration-config.js",
    "save-routes-db": "SAVE_ROUTES=true node dist/main.js",
    "test:module": "npx jest src/applications/$npm_config_module",
    "prepare-husky": "husky install",
    "release": "npm run test:unit && npx cz --allow-empty && npx semantic-release"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.758.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.0.11",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "amqp-connection-manager": "^4.1.14",
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "nodemailer": "^6.10.0",
    "npm": "^11.2.0",
    "npm-check-updates": "^17.1.15",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@jest-mock/express": "^3.0.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/express": "^5.0.0",
    "@types/faker": "^6.6.8",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "commit-msg-linter": "^1.1.0",
    "commitizen": "^4.3.1",
    "coveralls": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "16.4.7",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "jest-mock-extended": "^3.0.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
